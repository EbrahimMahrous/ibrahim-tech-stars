{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "text-wrap",
          "text-wrap: balance",
          "text-wrap: pretty"
        ]
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off",
        "aria-valid-attr-value": "off",
        "aria-required-children": "off"
      }
    ],
    "no-inline-styles": "off",
    "typescript-config/consistent-casing": "off",
    "axe/language": [
      "default",
      {
        "valid-lang": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ]
  }
}